{
  "name": "volder",
  "version": "1.1.2",
  "description": "schema builder and validating",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "dev": "nodemon test.js",
    "build": "tsdx build",
    "test": "tsdx test --coverage",
    "lint": "tsdx lint src test",
    "format": "tsdx lint src test --fix",
    "prepare": "husky install",
    "publish": "npm publish --access public"
  },
  "homepage": "https://github.com/devSupporters/volder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devSupporters/volder.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "keywords": [
    "schema-builder",
    "validator"
  ],
  "author": "salah alhashmi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devSupporters/volder/issues"
  },
  "devDependencies": {
    "husky": "^7.0.2"
  }
}
