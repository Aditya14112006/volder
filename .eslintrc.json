{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-new-wrappers":"off",
        "no-new-object":"off",
        "no-prototype-builtins":"off"
    }
}
